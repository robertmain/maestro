{
  "name": "jukebox-backend",
  "version": "1.0.0",
  "description": "Backend for node jukebox",
  "scripts": {
    "build": "webpack",
    "start:server:dev": "nodemon",
    "start:client:dev": "webpack-dev-server --config webpack.dev.config.js",
    "lint": "eslint src/**/*.{ts,vue}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "orm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "author": "Robert Main",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:robertmain/jukebox.git"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^1.2.0",
    "@nestjs/common": "^8.0.11",
    "@nestjs/core": "^8.0.11",
    "@nestjs/platform-express": "^8.0.11",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/typeorm": "^8.0.2",
    "axios": "^0.22.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@nestjs/testing": "^8.0.11",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.18",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/typescript-estree": "^4.33.0",
    "babel-loader": "^8.2.2",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
